<div id="{{rootid}}" class="aulasaovivo" data-region="aulasaovivo" data-config='{{{configjson}}}'>
    <div class="aulasaovivo__notice" data-region="fallback-notice" role="status" aria-live="polite" hidden>
        <span>{{#str}}fallbacknotice, local_aulasaovivo{{/str}}</span>
    </div>
    <div class="aulasaovivo__feedback" data-region="feedback" aria-live="polite"></div>
    <div class="aulasaovivo__tabs" role="tablist" aria-label="{{#str}}tablistlabel, local_aulasaovivo{{/str}}">
        <button type="button"
                class="aulasaovivo__tab"
                id="{{rootid}}-tab-catalog"
                data-action="switch-panel"
                data-target="catalog"
                role="tab"
                aria-selected="true"
                aria-controls="{{rootid}}-panel-catalog">
            {{#str}}tabcatalog, local_aulasaovivo{{/str}}
        </button>
        <button type="button"
                class="aulasaovivo__tab"
                id="{{rootid}}-tab-enrolled"
                data-action="switch-panel"
                data-target="enrolled"
                role="tab"
                aria-selected="false"
                aria-controls="{{rootid}}-panel-enrolled"
                tabindex="-1">
            {{#str}}tabenrolled, local_aulasaovivo{{/str}}
        </button>
        <button type="button"
                class="aulasaovivo__tab"
                id="{{rootid}}-tab-certificates"
                data-action="switch-panel"
                data-target="certificates"
                role="tab"
                aria-selected="false"
                aria-controls="{{rootid}}-panel-certificates"
                tabindex="-1">
            {{#str}}tabcertificates, local_aulasaovivo{{/str}}
        </button>
    </div>
    <div class="aulasaovivo__dashboard" data-region="dashboard">
        <section class="aulasaovivo__panel"
                 data-panel="catalog"
                 id="{{rootid}}-panel-catalog"
                 role="tabpanel"
                 aria-labelledby="{{rootid}}-tab-catalog"
                 aria-hidden="false"
                 tabindex="0">
            <header class="aulasaovivo__panel-header">
                <div class="aulasaovivo__panel-titles">
                    <span class="aulasaovivo__panel-title">{{#str}}catalogtitle, local_aulasaovivo{{/str}}</span>
                    <span class="aulasaovivo__panel-subtitle">{{#str}}catalogsubtitle, local_aulasaovivo{{/str}}</span>
                </div>
                <button type="button" class="aulasaovivo__refresh" data-action="refresh" data-target="catalog">
                    {{#str}}refresh, local_aulasaovivo{{/str}}
                </button>
            </header>
            <div class="aulasaovivo__panel-body">
                <div class="aulasaovivo__carousel" data-region="carousel">
                    <button type="button" class="aulasaovivo__nav aulasaovivo__nav--prev" data-action="scroll" data-direction="prev" aria-label="{{#str}}previous, local_aulasaovivo{{/str}}">
                        <span aria-hidden="true">&#10094;</span>
                    </button>
                    <div class="aulasaovivo__slides" data-region="cards"></div>
                    <button type="button" class="aulasaovivo__nav aulasaovivo__nav--next" data-action="scroll" data-direction="next" aria-label="{{#str}}next, local_aulasaovivo{{/str}}">
                        <span aria-hidden="true">&#10095;</span>
                    </button>
                </div>
                <div class="aulasaovivo__agenda">
                    <div class="aulasaovivo__agenda-header">
                        <h3 class="aulasaovivo__agenda-title">{{#str}}agendatitlecatalog, local_aulasaovivo{{/str}}</h3>
                    </div>
                    <div class="aulasaovivo__agenda-list" data-region="agenda"></div>
                </div>
            </div>
        </section>
        <section class="aulasaovivo__panel"
                 data-panel="enrolled"
                 id="{{rootid}}-panel-enrolled"
                 role="tabpanel"
                 aria-labelledby="{{rootid}}-tab-enrolled"
                 aria-hidden="true"
                 tabindex="-1"
                 hidden>
            <header class="aulasaovivo__panel-header">
                <div class="aulasaovivo__panel-titles">
                    <span class="aulasaovivo__panel-title">{{#str}}enrolledtitle, local_aulasaovivo{{/str}}</span>
                    <span class="aulasaovivo__panel-subtitle">{{#str}}enrolledsubtitle, local_aulasaovivo{{/str}}</span>
                </div>
                <button type="button" class="aulasaovivo__refresh" data-action="refresh" data-target="enrolled">
                    {{#str}}refresh, local_aulasaovivo{{/str}}
                </button>
            </header>
            <div class="aulasaovivo__panel-body">
                <div class="aulasaovivo__carousel" data-region="carousel">
                    <button type="button" class="aulasaovivo__nav aulasaovivo__nav--prev" data-action="scroll" data-direction="prev" aria-label="{{#str}}previous, local_aulasaovivo{{/str}}">
                        <span aria-hidden="true">&#10094;</span>
                    </button>
                    <div class="aulasaovivo__slides" data-region="cards"></div>
                    <button type="button" class="aulasaovivo__nav aulasaovivo__nav--next" data-action="scroll" data-direction="next" aria-label="{{#str}}next, local_aulasaovivo{{/str}}">
                        <span aria-hidden="true">&#10095;</span>
                    </button>
                </div>
                <div class="aulasaovivo__agenda">
                    <div class="aulasaovivo__agenda-header">
                        <h3 class="aulasaovivo__agenda-title">{{#str}}agendatitleenrolled, local_aulasaovivo{{/str}}</h3>
                    </div>
                    <div class="aulasaovivo__agenda-list" data-region="agenda"></div>
                </div>
            </div>
        </section>
        <section class="aulasaovivo__panel"
                 data-panel="certificates"
                 id="{{rootid}}-panel-certificates"
                 role="tabpanel"
                 aria-labelledby="{{rootid}}-tab-certificates"
                 aria-hidden="true"
                 tabindex="-1"
                 hidden>
            <header class="aulasaovivo__panel-header">
                <div class="aulasaovivo__panel-titles">
                    <span class="aulasaovivo__panel-title">{{#str}}certificatestitle, local_aulasaovivo{{/str}}</span>
                    <span class="aulasaovivo__panel-subtitle">{{#str}}certificatessubtitle, local_aulasaovivo{{/str}}</span>
                </div>
                <button type="button" class="aulasaovivo__refresh" data-action="refresh" data-target="certificates">
                    {{#str}}refresh, local_aulasaovivo{{/str}}
                </button>
            </header>
            <div class="aulasaovivo__panel-body">
                <div class="aulasaovivo__certificates" data-region="certificates"></div>
            </div>
        </section>
    </div>
</div>
