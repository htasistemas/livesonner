<div id="{{rootid}}" class="block-painelaulas" data-region="painelaulas">
    <div class="block-painelaulas__notice" data-region="fallback-notice" role="status" aria-live="polite" hidden>
        <span>{{strings.fallbacknotice}}</span>
    </div>
    <div class="block-painelaulas__feedback" data-region="feedback" aria-live="polite"></div>
    <div class="block-painelaulas__dashboard" data-region="dashboard">
        <section class="block-painelaulas__panel" data-panel="catalog">
            <header class="block-painelaulas__panel-header">
                <div class="block-painelaulas__panel-titles">
                    <span class="block-painelaulas__panel-title">{{strings.catalogtitle}}</span>
                    <span class="block-painelaulas__panel-subtitle">{{strings.catalogsubtitle}}</span>
                </div>
                <button type="button" class="block-painelaulas__refresh" data-action="refresh" data-target="catalog">
                    {{strings.refresh}}
                </button>
            </header>
            <div class="block-painelaulas__panel-body">
                <div class="block-painelaulas__carousel" data-region="carousel">
                    <button type="button" class="block-painelaulas__nav block-painelaulas__nav--prev" data-action="scroll" data-direction="prev" aria-label="{{strings.previous}}">
                        <span aria-hidden="true">&#10094;</span>
                    </button>
                    <div class="block-painelaulas__slides" data-region="cards"></div>
                    <button type="button" class="block-painelaulas__nav block-painelaulas__nav--next" data-action="scroll" data-direction="next" aria-label="{{strings.next}}">
                        <span aria-hidden="true">&#10095;</span>
                    </button>
                </div>
                <div class="block-painelaulas__agenda">
                    <div class="block-painelaulas__agenda-header">
                        <h3 class="block-painelaulas__agenda-title">{{strings.agendatitlecatalog}}</h3>
                    </div>
                    <div class="block-painelaulas__agenda-list" data-region="agenda"></div>
                </div>
            </div>
        </section>
        <section class="block-painelaulas__panel" data-panel="enrolled">
            <header class="block-painelaulas__panel-header">
                <div class="block-painelaulas__panel-titles">
                    <span class="block-painelaulas__panel-title">{{strings.enrolledtitle}}</span>
                    <span class="block-painelaulas__panel-subtitle">{{strings.enrolledsubtitle}}</span>
                </div>
                <button type="button" class="block-painelaulas__refresh" data-action="refresh" data-target="enrolled">
                    {{strings.refresh}}
                </button>
            </header>
            <div class="block-painelaulas__panel-body">
                <div class="block-painelaulas__carousel" data-region="carousel">
                    <button type="button" class="block-painelaulas__nav block-painelaulas__nav--prev" data-action="scroll" data-direction="prev" aria-label="{{strings.previous}}">
                        <span aria-hidden="true">&#10094;</span>
                    </button>
                    <div class="block-painelaulas__slides" data-region="cards"></div>
                    <button type="button" class="block-painelaulas__nav block-painelaulas__nav--next" data-action="scroll" data-direction="next" aria-label="{{strings.next}}">
                        <span aria-hidden="true">&#10095;</span>
                    </button>
                </div>
                <div class="block-painelaulas__agenda">
                    <div class="block-painelaulas__agenda-header">
                        <h3 class="block-painelaulas__agenda-title">{{strings.agendatitleenrolled}}</h3>
                    </div>
                    <div class="block-painelaulas__agenda-list" data-region="agenda"></div>
                </div>
            </div>
        </section>
    </div>
</div>
